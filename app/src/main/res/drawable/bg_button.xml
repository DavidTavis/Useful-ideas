<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#CE93D0" />
            <corners
                android:radius="10dp" />

        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:startColor="#25CE93D8"
                android:endColor="#60CE93D8"
                android:angle="270" />
            <corners
                android:radius="10dp" />

        </shape>
    </item>

</selector>